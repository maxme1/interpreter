class Point {
    def init(x, y){
        this.x = x;
        this.y = y;
    }

    def plus(that){
        return Point(this.x + that.x, this.y + that.y);
    }

    def str(){
        return '(' + String(this.x) + ', ' + String(this.y) + ')';
    }
}

var a = Point(0, 1);
var b = Point(10, 20);

print(a + b);
print(a.plus(b));